<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.dred" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>dred.</name>
    <description>
        A task and event tracking app based in CBT designed to help users manage their anxiety
    </description>
    <author email="scott.r.rothman@gmail.com" href="http://fidgit.io">
        Scott Rothman
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="img/icon.png" />
</widget>
